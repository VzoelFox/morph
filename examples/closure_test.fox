fungsi makeAdder(x int) fungsi(int) int {
  kembalikan fungsi(y int) int {
    kembalikan x + y
  }
}

fungsi main() {
  var add5 = makeAdder(5)
  var result = add5(10)

  assert(result == 15, "Result must be 15")
  native_print_int(result)
}

fungsi assert(cond bool, msg string) {
  jika !cond {
    native_print(msg)
    // panic?
  }
}
