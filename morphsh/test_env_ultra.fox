# Ultra Minimal Environment Test
# Hardcoded variables untuk avoid string comparison overhead

struktur Env
    x int
    y int
    z int
akhir

fungsi env_new() Env
    kembalikan Env(0, 0, 0)
akhir

fungsi env_set_x(e Env, val int) Env
    e.x = val
    kembalikan e
akhir

fungsi env_set_y(e Env, val int) Env
    e.y = val
    kembalikan e
akhir

fungsi env_set_z(e Env, val int) Env
    e.z = val
    kembalikan e
akhir

fungsi main() void
    native_print("=== ENV TEST ===")
    
    var e = env_new()
    e = env_set_x(e, 42)
    
    jika e.x == 42
        native_print("x=42: OK")
    akhir
    
    e = env_set_y(e, 10)
    e = env_set_z(e, 20)
    
    var sum = e.y + e.z
    jika sum == 30
        native_print("y+z=30: OK")
    akhir
    
    native_print("=== PASSED ===")
akhir

main()
