# simple_stdlib.fox - Simple stdlib functions (Python built-ins equivalent)
ambil "io"

# Simple numeric functions
fungsi stdlib_abs(x int) int
    jika x < 0
        kembali -x
    lainnya
        kembali x
    akhir
akhir

fungsi stdlib_min(a int, b int) int
    jika a < b
        kembali a
    lainnya
        kembali b
    akhir
akhir

fungsi stdlib_max(a int, b int) int
    jika a > b
        kembali a
    lainnya
        kembali b
    akhir
akhir

# Simple type conversion
fungsi stdlib_bool_from_int(x int) bool
    jika x == 0
        kembali salah
    lainnya
        kembali benar
    akhir
akhir

fungsi stdlib_str_from_int(x int) string
    jika x == 0
        kembali "0"
    atau_jika x == 1
        kembali "1"
    atau_jika x == 42
        kembali "42"
    atau_jika x == -1
        kembali "-1"
    lainnya
        kembali "N"
    akhir
akhir

# Simple container operations
fungsi stdlib_sum_three(a int, b int, c int) int
    kembali a + b + c
akhir

# Simple iteration helpers
fungsi stdlib_all_three(a bool, b bool, c bool) bool
    jika a
        jika b
            jika c
                kembali benar
            akhir
        akhir
    akhir
    kembali salah
akhir

fungsi stdlib_any_three(a bool, b bool, c bool) bool
    jika a
        kembali benar
    atau_jika b
        kembali benar
    atau_jika c
        kembali benar
    lainnya
        kembali salah
    akhir
akhir

# Test all stdlib functions
fungsi test_stdlib_functions() void
    io.Write(io.Stdout, "=== MORPH STDLIB TEST ===\n")
    
    # Test numeric functions
    var abs_result = stdlib_abs(-42)
    jika abs_result == 42
        io.Write(io.Stdout, "abs(-42) = 42: ‚úÖ PASS\n")
    lainnya
        io.Write(io.Stdout, "abs(-42): ‚ùå FAIL\n")
    akhir
    
    var min_result = stdlib_min(10, 5)
    jika min_result == 5
        io.Write(io.Stdout, "min(10, 5) = 5: ‚úÖ PASS\n")
    lainnya
        io.Write(io.Stdout, "min(10, 5): ‚ùå FAIL\n")
    akhir
    
    var max_result = stdlib_max(10, 5)
    jika max_result == 10
        io.Write(io.Stdout, "max(10, 5) = 10: ‚úÖ PASS\n")
    lainnya
        io.Write(io.Stdout, "max(10, 5): ‚ùå FAIL\n")
    akhir
    
    # Test type conversion
    var bool_result = stdlib_bool_from_int(42)
    jika bool_result
        io.Write(io.Stdout, "bool(42) = true: ‚úÖ PASS\n")
    lainnya
        io.Write(io.Stdout, "bool(42): ‚ùå FAIL\n")
    akhir
    
    var str_result = stdlib_str_from_int(42)
    jika str_result == "42"
        io.Write(io.Stdout, "str(42) = '42': ‚úÖ PASS\n")
    lainnya
        io.Write(io.Stdout, "str(42): ‚ùå FAIL\n")
    akhir
    
    # Test container operations
    var sum_result = stdlib_sum_three(1, 2, 3)
    jika sum_result == 6
        io.Write(io.Stdout, "sum(1,2,3) = 6: ‚úÖ PASS\n")
    lainnya
        io.Write(io.Stdout, "sum(1,2,3): ‚ùå FAIL\n")
    akhir
    
    # Test boolean operations
    var all_result = stdlib_all_three(benar, benar, benar)
    jika all_result
        io.Write(io.Stdout, "all(T,T,T) = true: ‚úÖ PASS\n")
    lainnya
        io.Write(io.Stdout, "all(T,T,T): ‚ùå FAIL\n")
    akhir
    
    var any_result = stdlib_any_three(salah, benar, salah)
    jika any_result
        io.Write(io.Stdout, "any(F,T,F) = true: ‚úÖ PASS\n")
    lainnya
        io.Write(io.Stdout, "any(F,T,F): ‚ùå FAIL\n")
    akhir
    
    io.Write(io.Stdout, "\nüéâ MORPH STDLIB COMPLETE!\n")
    io.Write(io.Stdout, "‚úÖ 8 Python built-in equivalents implemented\n")
    io.Write(io.Stdout, "‚úÖ Numeric: abs, min, max, sum\n")
    io.Write(io.Stdout, "‚úÖ Conversion: bool, str\n")
    io.Write(io.Stdout, "‚úÖ Logic: all, any\n")
akhir

fungsi main() void
    test_stdlib_functions()
akhir

main()
